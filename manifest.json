{
	"manifest_version": 2,
	"name": "KeepZotero",
	"version": "0.2.0",
	"description": "Keep Zotero from accidental close",
	"homepage_url": "https://github.com/yhmtsai/KeepZotero",
	"applications": {
		"zotero": {
			"id": "keepzotero@yhmtsai",
